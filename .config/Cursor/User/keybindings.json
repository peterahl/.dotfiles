[
    {
        "key": "space f f",
        "command": "workbench.action.quickOpen",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+i",
        "command": "workbench.action.navigateForward",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space s s",
        "command": "actions.find",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space s p",
        "command": "workbench.action.findInFiles",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space f r",
        "command": "workbench.action.openRecent",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space s g",
        "command": "workbench.action.findInFiles",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space b b",
        "command": "workbench.action.showAllEditors",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space b d",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w v",
        "command": "workbench.action.splitEditor",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w s",
        "command": "workbench.action.splitEditorOrthogonal",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w c",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w h",
        "command": "workbench.action.focusLeftGroup",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen && !inQuickFixWidget"
    },
    {
        "key": "ctrl+n",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "ctrl+p",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "alt+x",
        "command": "workbench.action.showCommands",
        "when": "editorTextFocus"
    },
    {
        "key": "space c f",
        "command": "editor.action.formatDocument",
        "when": "editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
    },
    {
        "key": "space c a",
        "command": "editor.action.quickFix",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space c r",
        "command": "editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
    },
    {
        "key": "space e",
        "command": "editor.action.showHover",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w l",
        "command": "workbench.action.focusRightGroup",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w j",
        "command": "workbench.action.focusBelowGroup",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w k",
        "command": "workbench.action.focusAboveGroup",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space ,",
        "command": "workbench.action.quickOpen",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space g s",
        "command": "workbench.view.scm",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space w h",
        "command": "workbench.action.focusSideBar",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "g r",
        "command": "editor.action.goToReferences",
        "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && vim.mode != 'Insert'"
    },
    {
        "key": "g d",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor && vim.mode != 'Insert'"
    },
    {
        "key": "g S",
        "command": "workbench.action.showAllSymbols",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space a t",
        "command": "workbench.action.terminal.focus",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "space p p",
        "command": "workbench.action.openRecent",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    },
    {
        "key": "g c",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+q",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarVisible"
    },
    {
        "key": "ctrl+w q",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "sideBarFocus"
    },
    {
        "key": "space f s",
        "command": "workbench.action.files.save",
        "when": "editorTextFocus && !editorReadonly && vim.mode != 'Insert'"
    },
    {
        "key": "g g",
        "command": "cursorTop",
        "when": "editorTextFocus && vim.mode != 'Insert'"
    }
]